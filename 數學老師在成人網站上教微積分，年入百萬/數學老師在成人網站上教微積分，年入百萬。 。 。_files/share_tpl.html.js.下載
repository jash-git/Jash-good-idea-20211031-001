"function"==typeof n&&n.apply(void 0,f);
}),i[e]&&(i[e].forEach(function(n){
"function"==typeof n&&n.apply(void 0,f);
}),n(e));
}
function f(n,e,t){
if("function"==typeof e)if(t&&o[n]){
var f=o[n].indexOf(e);
-1!==f&&o[n].splice(f,1);
}else if(i[n]){
var f=i[n].indexOf(e);
-1!==f&&i[n].splice(f,1);
}
}
var i={},o={};
return{
on:e,
emit:t,
remove:f,
clean:n
};
